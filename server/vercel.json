{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",   // حدد المسار إلى الملف الرئيسي
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",    // كل الطلبات ستذهب إلى الملف الرئيسي
      "dest": "/app.js"  // المسار الذي يوجه كل الطلبات إليه
    }
  ]
    }
    
